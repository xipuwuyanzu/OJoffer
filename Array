public class Solution {
    public boolean Find(int target, int [][] array) {
      int rowNum ;
      int colNum ;
      boolean flag =false;
      rowNum = array.length;
      if(rowNum >0){
           colNum = array[0].length;
                for(int i=0;i<rowNum;i++){
            
            for(int j = 0;j<colNum;j++){
                
                if(target==array[i][j]){
                    flag = true;
                    return flag;
                }
                
            }
            
        }
      }
       

        return flag;
    }
}
